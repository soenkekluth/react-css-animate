{
  "presets": [
    ["es2015", {"loose": true}],
    "react",
    "stage-0"
  ],
  "plugins": [
    [
      "transform-runtime", {
        "helpers": true,
        "polyfill": false,
        "regenerator": false,
        "moduleName": "babel-runtime"
      }
    ],
    "transform-class-properties",
    "add-module-exports"
  ]
}
